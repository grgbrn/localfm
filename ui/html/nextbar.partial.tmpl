{{define "nextbar"}}
<div class="datebar">
  <div class="datebar-title">
    {{.Title}}: <span id="datebar-title-label"></span>
  </div>
  <div class="datebar-controls">
    {{ if (ne .Previous "") }}
    <div>
      <a id="prevlink" href="{{.Previous}}">&#8592; Previous</a>
    </div>
    {{ end }}

    {{ if (ne .Next "") }}
    <div>
      <a id="nextlink" href="{{.Next}}">Next &#8594;</a>
    </div>
    {{ end }}
  </div>
</div>
{{end}}